// Bubble short
#include<iostream.h>
#include<conio.h>
void main()
{
	clrscr();
	int i,j,x,n,a[50];
	cout<<"Enter the limit",,endl;
	cin>>n;
	cuot<<"Enter the elements "<<endl;
	for(i=0;i<n;i++)
	cin>>a[i];
	for(j=0;j<n;j++)
	{
		for(i=0;i<n;i++)
		{
			if(a[i]>a[i+1])
			{
				x=a[i+1];
				a[i+1]=a[i];
				a[i]=x;
			}
		}
	}
	cout<<"Result"<<endl;
	for(i=0;i<n;i++)
	{
		cout<<a[i];
		cout<<endl;
	}
	getch();
	
}
